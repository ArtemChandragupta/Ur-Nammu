<Interface>:
    cols: 2
    padding: 10
    spacing: 10

    Button:
        text: 'Touch xlsx'
        on_press: root.excel()

    Label:
        text: 'Address A:'

    TextInput:
        id: input_a
        multiline: False
        on_text: root.on_a_text(self, self.text)

    Label:
        text: root.adress_a

    Label:
        text: 'Address B:'

    TextInput:
        id: input_b
        multiline: False
        on_text: root.on_b_text(self, self.text)

    Label:
        text: root.adress_b

